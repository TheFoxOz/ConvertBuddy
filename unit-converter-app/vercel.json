{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0, stale-while-revalidate"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "VERCEL_PROJECT_ID": "unit-converter-app"
    }
  },
  "installCommand": "echo 'No install needed for static project'",
  "buildCommand": "echo 'No build command needed for static project'",
  "outputDirectory": "."
}
